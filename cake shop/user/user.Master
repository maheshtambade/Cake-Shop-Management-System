      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <!-- Navbar links on the right side -->
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="../user/home.aspx">Home</a>
          </li>
             <li class="nav-item">
             <a class="nav-link" href="../user/shop.aspx">Shop</a>
             </li>
          <li class="nav-item">
            <a class="nav-link" href="../user/myorders.aspx">My Orders</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="../user/about.aspx">About Us</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="../user/contact.aspx">Contact</a>
          </li>
             <li class="nav-item">
       <a class="nav-link" href="../user/notification.aspx"><i class="fa-solid fa-bell"></i> Notify</a>
         </li>
            
            <li class="nav-item">
                
           <a class="nav-link" href="#"> Welcome, <asp:Literal ID="lblUsername" runat="server"></asp:Literal>!</a>
        
            </li>
            <li class="nav-item">
                <asp:Button ID="Button1" runat="server" Text="Logout" CssClass="btn btn-danger btn-small" OnClick="btnLogout_Click" />
            </li>
        </ul>
      </div>
    </div>
  </nav>

  






  
  
   




   
