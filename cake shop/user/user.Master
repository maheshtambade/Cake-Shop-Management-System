  <style>
    /* Custom Styles */
    .navbar {
      background-color: #333; /* Light black background */
    }

    .navbar-brand {
      font-size: 24px;
      font-weight: bold;
      color: white;
      text-transform: uppercase;
      letter-spacing: 2px;
    }

    .navbar-nav .nav-link {
      color: white !important;
      font-size: 16px;
      font-weight: normal;
    }

    .navbar-nav .nav-link:hover {
      color: #f8c9b6 !important; /* Light cream color on hover */
    }

    /* Optional: Add margin to the navbar to give some breathing space */
    .navbar {
      margin-bottom: 20px;
    }



     <!-- Custom CSS for navbar and footer -->
 
     .navbar {
         background-color: black; /* Light background */
     }
     .navbar-nav .nav-link {
         color: white; /* Dark text */
     }
     .navbar-nav .nav-link:hover {
         color: #007bff; /* Hover effect */
     }

     .footer {
         background-color: black;
         padding: 20px 0;
         margin-top: 50px;
         color: white;
     }

     .footer .social-links a {
         margin: 0 15px;
         color: white;
         font-size: 1.5em; /* Larger icons */
         text-decoration: none;
     }

     .footer .social-links a:hover {
         color: #007bff; /* Hover effect */
     }
 
  </style>
</head>
<body>
     <form id="form1" runat="server">
  <!-- Navbar -->
